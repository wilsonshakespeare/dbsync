<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.research.dbsync">

    <application
        android:name=".MainDriver"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".project.screen.user.UserFormActivity">
        </activity>

        <activity android:name=".project.screen.user.UserListActivity">
        </activity>

        <activity android:name=".project.screen.task.TaskFormActivity">
        </activity>

        <activity android:name=".project.screen.task.TaskListActivity">
        </activity>
    </application>

</manifest>